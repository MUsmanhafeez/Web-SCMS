{
  "name": "amoxt-app-plan-backend",
  "version": "0.0.1",
  "description": "Project description goes here",
  "author": "Amoxt Dev Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "pre-commit": "run-p format lint",
    "pre-push": "NODE_ENV=test run-p test:lint",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV=production node idst/main",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\" \"tests/**/**.ts\" --fix",
    "test": "npm run build && run-p test:lint",
    "test:lint": "eslint \"{src,apps,libs}/**/*.ts\" \"tests/**/**.ts\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./tests/jest-e2e.json",
    "test:e2e:prod": "NODE_ENV=production jest --config ./tests/jest-e2e-prod.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@nestjs/common": "^8.0.0-alpha.4",
    "@nestjs/core": "^8.0.0-alpha.4",
    "@nestjs/graphql": "^7.10.6",
    "@nestjs/microservices": "^8.0.0-alpha.4",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^8.0.0-alpha.4",
    "@nestjs/typeorm": "^7.1.5",
    "@ouato/nestjs-express-cassandra": "^6.0.1",
    "apollo-server-express": "^2.19.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.12.2",
    "graphql-tools": "^7.0.1",
    "ioredis": "^4.24.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "nats": "^2.0.2",
    "oidc-provider": "^7.1.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "randomstring": "^1.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.2",
    "@nestjs/schematics": "^7.2.1",
    "@nestjs/testing": "^8.0.0-alpha.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.26.1",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "apollo-server-testing": "^2.19.2",
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-nestjs": "^1.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.0",
    "jest": "26.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@lib/graphql/(.*)": "<rootDir>/libs/graphql/src/$1",
      "@lib/graphql": "<rootDir>/libs/graphql/src"
     }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-push"
    }
  }
}
